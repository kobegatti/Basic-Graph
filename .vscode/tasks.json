{
    "tasks": [
        {
            "type": "shell",
            "label": "build",
            "command": "/usr/bin/g++",
            "args": [
                "-std=c++20",
                "-Wall",
                "-Werror",
                "-Wpedantic",
                "-fdiagnostics-color=always",
                "-g",
                "-I/${workspaceFolder}/header/external/nlohmann",
                "\"${workspaceFolder}/src/basicGraph.cpp\"",
                "\"${workspaceFolder}/src/utils.cpp\"",
                "\"${workspaceFolder}/src/main.cpp\"",
                "-o",
                "${workspaceFolder}/main"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}